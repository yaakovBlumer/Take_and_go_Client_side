<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">



        <TableLayout
            android:layout_width="368dp"
            android:layout_height="296dp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="17dp">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" />


            <TextView
                android:id="@+id/customerNum"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Customer Number:"
                android:textColor="@color/colorDarkGreen"
                android:textSize="18dp"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="30dp" />

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" />


            <TextView
                android:id="@+id/modeOfOrder"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Mode Of Order:"
                android:textColor="@color/colorDarkGreen"
                android:textSize="18dp"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="30dp" />

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" />


            <TextView
                android:id="@+id/carNumber"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Car Number:"
                android:textColor="@color/colorDarkGreen"
                android:textSize="18dp"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="30dp" />

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" />


            <TextView
                android:id="@+id/rentStartDate"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Rent Start Date:"
                android:textColor="@color/colorDarkGreen"
                android:textSize="18dp"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="30dp" />

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" />


            <TextView
                android:id="@+id/rentEndDate"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Rent End Date:"
                android:textColor="@color/colorDarkGreen"
                android:textSize="18dp"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="30dp" />


            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

            <TextView
                android:id="@+id/kilometresAtStart"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Kilometres At Start:"
                android:textColor="@color/colorDarkGreen"
                android:textSize="18dp"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="30dp" />

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

            <TextView
                android:id="@+id/kilometresAtEnd"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Kilometres At End:"
                android:textColor="@color/colorDarkGreen"
                android:textSize="18dp"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="30dp" />

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

            <TextView
                android:id="@+id/isInsertDelek"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Is Insert Delek:"
                android:textColor="@color/colorDarkGreen"
                android:textSize="18dp"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="30dp" />

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

            <TextView
                android:id="@+id/howMuchDelekInsert"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="How Much Delek Insert:"
                android:textColor="@color/colorDarkGreen"
                android:textSize="18dp"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="30dp" />

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

            <TextView
                android:id="@+id/howMuchNeedPay"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="How Much Need Pay:"
                android:textColor="@color/colorDarkGreen"
                android:textSize="18dp"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="55dp" />

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

            <TextView
                android:id="@+id/orderNum"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Order Number:"
                android:textColor="@color/colorDarkGreen"
                android:textSize="18dp"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="80dp" />

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

        </TableLayout>


</android.support.constraint.ConstraintLayout>